{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    },
    {
      "src": "hippo.png",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server.js"
    },
    {
      "src": "/hippo.png",
      "dest": "/hippo.png"
    },
    {
      "src": "/",
      "dest": "/server.js"
    }
  ]
}
